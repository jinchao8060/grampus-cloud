<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oceancloud.grampus.admin.modules.system.dao.SysDictDao">

	<select id="getSysDictList" resultType="com.oceancloud.grampus.admin.modules.system.entity.SysDict">
		SELECT sd.id, sd.dict_type FROM sys_dict sd ORDER BY sd.dict_type, sd.sort
	</select>
</mapper>